# Facebook Messenger Bot Environment Variables
# Copy this file to .env and fill in your actual values

# Your Facebook Page Access Token (get this from Facebook Developer Console)
FB_PAGE_ACCESS_TOKEN=your_facebook_page_access_token_here

# Optional: Custom verify token for webhook (default is 'my_voice_is_my_password_verify_me')
WEBHOOK_VERIFY_TOKEN=my_voice_is_my_password_verify_me

# Optional: Port for local development (default is 5000)
PORT=5000 